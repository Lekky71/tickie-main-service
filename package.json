    "express-validator": "6.15.0",
    "faker": "4.1.0",
    "helmet": "3.21.2",
    "log4js": "6.7.1",
    "memory-cache": "0.2.0",
    "method-override": "3.0.0",
    "moment": "2.29.4",
    "request-debug": "0.2.0",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "4.6.2",
    "uuid": "3.3.3"
  },
