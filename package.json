{
  "name": "example-app",
  "version": "1.0.0",
  "description": "NodeJs API Typescript",
  "main": "./src/bin/www/index.ts",
  "private": true,
  "scripts": {
    "prod:start": "NODE_ENV=production node ./dist/bin/www/index.js",
    "dev:start": "NODE_ENV=development ts-node ./src/bin/www/index.ts",
    "dev:watch": "nodemon ./src/bin/www/index.ts",
    "build": "webpack",
    "test": "jest",
    "test:watch": "jest --watchAll --collectCoverage=false",
    "test:vulnerability": "npm audit",
    "prettier:watch": "onchange './src/**/*.ts' -- prettier --write {{changed}}",
    "lint": "eslint './src/**/*.ts' --max-warnings 0",
    "clean": "rm -rf coverage dist",
    "typeorm": "ts-node ./node_modules/.bin/typeorm -f ./src/typeorm.ts"
  },
  "engines": {
    "node": ">=12.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lekky71/nodejs-api-typescript-template.git"
  },
  "keywords": [
    "NodeJs API",
    "ExpressJs",
    "Typescript"
  ],
  "author": {
    "name": "Lekky71",
    "email": "hashilekky@gmail.com"
  },
  "license": "LicenseRef-LICENSE",
  "bugs": {
    "url": "https://github.com/Lekky71/nodejs-api-typescript-template.git/issues"
  },
  "homepage": "https://github.com/Lekky71/nodejs-api-typescript-template.git#readme",
